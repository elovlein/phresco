				<staticText>
					<reportElement x="2" y="4" width="191" height="16" forecolor="#FFFFFF" backcolor="#000000"/>
					<textElement>
						<font fontName="Times New Roman"/>
					</textElement>
					<text><![CDATA[Â© Copyright Photon Infotech Pvt Ltd 2012.]]></text>
				</staticText>
			</frame>
		</band>
	</pageFooter>
</jasperReport>
